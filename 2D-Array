//Two dimesnsional array
import java.util.ArrayList;

public class MyClass {
  public static void main(String[] args) {
    // Create a 2D arraylist of integers
    ArrayList<ArrayList<Integer>> twoDArray = new ArrayList<ArrayList<Integer>>();

    // Add elements to the 2D arraylist
    ArrayList<Integer> row1 = new ArrayList<Integer>();
    row1.add(1);
    row1.add(2);
    row1.add(3);
    twoDArray.add(row1);

    ArrayList<Integer> row2 = new ArrayList<Integer>();
    row2.add(4);
    row2.add(5);
    row2.add(6);
    twoDArray.add(row2);

    ArrayList<Integer> row3 = new ArrayList<Integer>();
    row3.add(7);
    row3.add(8);
    row3.add(9);
    twoDArray.add(row3);

    // Accessing elements in the 2D arraylist
    System.out.println(twoDArray.get(0).get(1)); // prints 1
    System.out.println(twoDArray.get(1).get(2)); // prints 6
    System.out.println(twoDArray.get(2).get(1)); // prints 8
  }
}
/*
  2
  6
  8
*/
====

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.HttpHeaders;
import org.springframework.http.MediaType;
import org.springframework.http.ResponseEntity;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

import java.io.ByteArrayInputStream;

@RestController
public class PdfController {

    @Autowired
    private PdfService pdfService;

    @GetMapping("/waybill/pdf")
    public ResponseEntity<byte[]> downloadWaybillPdf() {
        ByteArrayInputStream bis = pdfService.generateWaybillPdf();

        return ResponseEntity.ok()
                .header(HttpHeaders.CONTENT_DISPOSITION, "attachment; filename=waybill.pdf")
                .contentType(MediaType.APPLICATION_PDF)
                .body(bis.readAllBytes());
    }
}